<configuration>
  <!-- Console Appender -->
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%msg%n</pattern>
    </encoder>
  </appender>

  <logger name="com.dtreb.library" level="debug"/>

  <!-- Root Logger -->
  <root level="info">
    <appender-ref ref="STDOUT"/>
  </root>
</configuration>